<h1>Listing Articles</h1>
<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
    </tr> 

    <% @articles.each do |article| %>
        <tr>
            <td><%= article.title %></td>
            <td><%= article.text %></td>
            <td><%= link_to 'Show', articles_path %></td>
        </tr>
    <% end %>
</table>